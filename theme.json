{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "custom": {
      "animation": "fade",
      "border": {
        "width": "2px",
        "style": "solid",
        "color": "var(--wp--preset--color--black)",
        "radius": "6px"
      },
      "boxShadow": {
        "x": "0em",
        "y": "7px",
        "blur": "0em",
        "spread": "-3px",
        "color": "var(--wp--preset--color--black)"
      },
      "navigationSeparator": {
        "content": ""
      }
    },
    "color": {
      "palette": [
        {
          "slug": "black",
          "name": "Black",
          "color": "#10152e"
        },
        {
          "slug": "white",
          "name": "white",
          "color": "#ffffff"
        }
      ],
      "gradients": [
        {
          "slug": "light",
          "name": "Light",
          "gradient": "linear-gradient(135deg, rgb(230, 255, 251) 0%, rgb(249, 249, 255) 14%, rgb(253, 255, 255) 24%, rgb(233, 232, 252) 35%, rgb(245, 240, 255) 45%, rgb(247, 255, 255) 57%, rgb(246, 255, 254) 67%, rgb(249, 248, 253) 82%, rgb(226, 222, 255) 100%)"
        },
        {
          "slug": "conic",
          "name": "Conic",
          "gradient": "var(--wp--custom--gradient--conic, conic-gradient(from 90deg, rgba(229, 245, 255), rgb(242, 255, 253), rgb(214, 255, 250), rgb(245, 254, 255), rgb(255, 234, 247), rgb(234, 226, 255), rgb(234, 252, 255), rgb(249, 246, 255), rgb(255, 254, 255), rgb(237, 236, 255), rgb(234, 252, 255), rgb(255, 237, 252), rgb(229, 245, 255)))"
        }
      ]
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--gradient--light)",
      "text": "var(--wp--preset--color--black)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--figtree)",
      "lineHeight": "1.6"
    },
    "elements": {
      "button": {
        "color": {
          "background": "var(--wp--preset--color--black)"
        }
      },
      "heading": {
        "typography": {
          "lineHeight": "1.3"
        }
      },
      "link": {
        "color": {
          "text": "inherit"
        }
      }
    }
  }
}
