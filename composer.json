{
  "name": "blockify/startify",
  "description": "Full site editing block starter theme for WordPress",
  "type": "wordpress-theme",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Lee Anthony",
      "email": "info@blockifywp.com"
    }
  ],
  "minimum-stability": "dev",
  "scripts": {
    "install:dev": "@composer update",
    "install:prod": "@composer update --no-dev",
    "install:local": "rm -Rf ./vendor/composer/installed.json && composer run install:prod",
    "install-codestandards": "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run",
    "phpcs": "phpcs",
    "pest": "pest"
  },
  "require": {
    "blockify/plugin": "@dev",
    "php": "^7.4 || ^8.0"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../plugins/blockify",
      "options": {
        "symlink": false
      }
    }
  ]
}
